{
  "name": "agri-backend",
  "version": "1.0.0",
  "description": "Agriculture Advisory Backend - Node.js API with Python ML Microservice",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "ml-service": "cd ml-service && python app.py",
    "seed": "node migrations/seedData.js",
    "migrate": "node migrations/seedData.js",
    "frontend": "cd frontend && PORT=3001 npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend\" \"npm run ml-service\"",
    "dev:web": "concurrently \"npm run dev\" \"npm run frontend\"",
    "build": "npm install",
    "postbuild": "npm run migrate",
    "prod": "NODE_ENV=production npm start"
  },
  "keywords": [
    "agriculture",
    "advisory",
    "crop-recommendation",
    "yield-prediction",
    "nodejs",
    "express",
    "ml"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^1.4.4",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  }
}
